{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "src/**/*.ts",
            "package.json",
            "tsconfig.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "MONGODB_URI": "@mongodb-uri",
      "JWT_SECRET": "@jwt-secret"
    }
}
  